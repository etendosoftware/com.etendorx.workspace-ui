{
  "name": "workspaceui",
  "version": "0.5.0",
  "description": "",
  "type": "module",
  "main": "packages/MainUI/server.js",
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "build:all": "pnpm --filter @workspaceui/storybook build && pnpm --filter @workspaceui/mainui build",
    "storybook": "pnpm --filter @workspaceui/storybook storybook",
    "dev": "turbo run dev --filter=@workspaceui/mainui",
    "build": "cd packages/MainUI && ./build.sh && cd ../..",
    "clean": "pnpm -r exec -- rm -rf dist node_modules .next && rm -rf node_modules",
    "start": "node dist",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --apply .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "node scripts/coverage.js",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:mainui": "jest --selectProjects MainUI",
    "check:data-testid": "sh ./scripts/check-add-data-testid.sh",
    "apply:data-testid": "sh ./scripts/apply-add-data-testid.sh",
    "test:api-client": "jest --selectProjects \"API Client\"",
    "test:component-library": "jest --selectProjects ComponentLibrary",
    "test:mainui:watch": "jest --selectProjects MainUI --watch",
    "test:api-client:watch": "jest --selectProjects \"API Client\" --watch",
    "test:component-library:watch": "jest --selectProjects ComponentLibrary --watch",
    "test:mainui:coverage": "cd packages/MainUI && npx jest --coverage --passWithNoTests --collectCoverageFrom='app/**/*.{ts,tsx}' --collectCoverageFrom='components/**/*.{ts,tsx}' --collectCoverageFrom='hooks/**/*.{ts,tsx}' --collectCoverageFrom='contexts/**/*.{ts,tsx}' --collectCoverageFrom='utils/**/*.{ts,tsx}' --collectCoverageFrom='screens/**/*.{ts,tsx}' --collectCoverageFrom='!**/*.test.{ts,tsx}' --collectCoverageFrom='!**/__tests__/**'",
    "test:api-client:coverage": "cd packages/api-client && npx jest --coverage --passWithNoTests --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom='!src/**/*.test.{ts,tsx}' --collectCoverageFrom='!src/**/__tests__/**'",
    "test:component-library:coverage": "cd packages/ComponentLibrary && npx jest --coverage --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom='!src/**/*.test.{ts,tsx}' --collectCoverageFrom='!src/**/__tests__/**'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "eslint": "^9.28.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^6.0.0",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "material-react-table": "2.13.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
