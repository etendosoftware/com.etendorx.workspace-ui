{
  "name": "workspaceui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:all": "pnpm --filter @workspaceui/storybook build && pnpm --filter @workspaceui/mainui build",
    "dev": "pnpm --filter @workspaceui/mainui dev",
    "build": "pnpm --filter @workspaceui/mainui build",
    "clean": "pnpm -r exec -- rm -rf dist node_modules .next && rm -rf node_modules",
    "start": "pnpm --filter @workspaceui/mainui start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "pnpm": {
    "hoistPattern": [
      "*"
    ]
  }
}
